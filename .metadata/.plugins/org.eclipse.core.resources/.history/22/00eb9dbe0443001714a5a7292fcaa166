 */
public class SysInfoControl {
public void ReadServer()
{
    BerkeleyDB db=new BerkeleyDB();
    db.open();
    db.openCursor();
    HashMap<byte[] , byte[]> hash=null;
    do{
          hash=db.getCursor();
          Iterator iter = hash.entrySet().iterator();
        　　while (iter.hasNext()) {
        　 entry =iter.next();
        　　Object key = entry.getKey();
        　　Object val = entry.getValue();
        　　}
    }while(hash.size()>0);
}
}
