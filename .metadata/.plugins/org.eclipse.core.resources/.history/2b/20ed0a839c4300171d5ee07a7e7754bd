    @Override
    public void recviceData(String src, byte[] data) {
        ByteBuffer buf=ByteBuffer.wrap(data);
        int idlen=buf.getInt();
        byte[]idbyte=new byte[idlen];
        buf.get(idbyte);
        String id=new String(idbyte);
        byte type=buf.get();
        if(type==0)
        {
            
        }
    }

    @Override
    public void setServer(Object socket) {
        
    }

}
